<?xml version="1.0" encoding="utf-8"?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup Condition=" '$(RunConfiguration)' == 'Default' ">
    <StartAction>Project</StartAction>
    <StartArguments>-target:library</StartArguments>
    <ExternalConsole>true</ExternalConsole>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(RunConfiguration)' == 'Default_Standalone' ">
    <StartAction>Project</StartAction>
    <StartArguments>-main:HSB.HSBStandalone</StartArguments>
    <ExternalConsole>true</ExternalConsole>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(RunConfiguration)' == 'Default_TestRunner' ">
    <StartAction>Project</StartAction>
    <StartArguments>-main:HSB.HSBStandalone</StartArguments>
    <ExternalConsole>true</ExternalConsole>
  </PropertyGroup>
</Project>